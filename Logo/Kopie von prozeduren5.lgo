lerne landschaft :a [ :y 150  ]
  setze "x (:y/10) 
  sh sxy (-:a) 0 sa
  wh 8
  [
    (haus :x)
    setze "x (:x-0.5)
    sh re 90 vw :x*6 li 90 vw :x*2 sa
  ]
