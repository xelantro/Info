lerne tunnel :l :o [ :k 91  ]
wenn (:o >= 0)
 [
   vw :l 
   re :k
   (tunnel (:l*0.9) (:o-1) :k)
   li :k
   rw :l
 ]
Ende


lerne quadrate :x :o
wenn (:o >= 0)
 [
  vw :x
  re 90
  vw :x

  li 90
  quadrate (:x*0.8) (:o-1)
  re 90

  re 90
  vw :x
  re 90
  vw :x
  re 90
]
Ende

lerne baum :x :o
wenn (:o >= 0)
 [
   vw :x
   li 45
   vw (:x/3)

   re 45
   baum (:x*0.5) (:o-1)
   li 45

   rw (:x/3)
   re 90
   vw (:x/3)

   li 45
   baum (:x*0.5) (:o-1)
   re 45

   rw (:x/3)
   li 45
   rw :x
 ]
Ende

lerne deiecke :x :o
wenn (:o >= 0)
 [
  vw :x

  re 360/1.5
  deiecke (:x*0.5) (:o-1)
  li 360/1.5

  re 360/3
  vw :x

  re 360/1.5
  deiecke (:x*0.5) (:o-1)
  li 360/1.5

  re 360/3
  vw :x

  re 360/1.5
  deiecke (:x*0.5) (:o-1)
  li 360/1.5

  re 360/3
  vw :x
]
Ende
